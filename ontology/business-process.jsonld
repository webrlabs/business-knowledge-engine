      "domain": "bke:Entity",
      "range": "xsd:dateTime",
      "owl:equivalentProperty": "schema:validFrom"
    },
    {
      "@id": "bke:validTo",
      "@type": "rdf:Property",
      "label": "valid to",
      "comment": "The date/time when this entity version expires or is no longer valid. Null indicates the entity is currently valid with no expiration.",
      "domain": "bke:Entity",
      "range": "xsd:dateTime",
      "owl:equivalentProperty": "schema:validThrough"
    },
    {
      "@id": "bke:supersededBy",
      "@type": "rdf:Property",
      "label": "superseded by",
      "comment": "Reference to the entity that replaces this version. Used when an entity is updated to track the successor version.",
      "domain": "bke:Entity",
      "range": "bke:Entity"
    },
    {
      "@id": "bke:supersedes",
      "@type": "rdf:Property",
      "label": "supersedes",
      "comment": "Reference to the previous entity version that this entity replaces. Inverse of supersededBy.",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:supersededBy"
    },
    {
      "@id": "bke:temporalStatus",
      "@type": "rdf:Property",
      "label": "temporal status",
      "comment": "The temporal status of the entity: 'current' (valid now), 'expired' (validTo has passed), 'pending' (validFrom is in the future), or 'superseded' (replaced by another version).",
      "domain": "bke:Entity",
      "range": "xsd:string",
      "bke:allowedValues": ["current", "expired", "pending", "superseded"]
    },
    {
      "@id": "bke:versionSequence",
      "@type": "rdf:Property",
      "label": "version sequence",
      "comment": "Monotonically increasing version number for this entity's lineage. Used to order entity versions chronologically.",
      "domain": "bke:Entity",
      "range": "xsd:integer"
    },

    {
      "@id": "bke:EntityVersionRelationship",
      "@type": "rdfs:Class",
      "label": "Entity Version Relationship",
      "comment": "Relationships for entity versioning and temporal modeling",
      "subClassOf": "bke:Relationship"
    },
    {
      "@id": "bke:SUPERSEDED_BY",
      "@type": "rdf:Property",
      "label": "superseded by",
      "comment": "Indicates that the source entity has been replaced by the target entity",
      "subClassOf": "bke:EntityVersionRelationship",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:SUPERSEDES"
    },
    {
      "@id": "bke:SUPERSEDES",
      "@type": "rdf:Property",
      "label": "supersedes",
      "comment": "Indicates that the source entity replaces the target entity",
      "subClassOf": "bke:EntityVersionRelationship",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:SUPERSEDED_BY"
    },

    {
      "@id": "bke:TemporalStatusDefinition",
      "@type": "rdfs:Class",
      "label": "Temporal Status Definition",
      "comment": "Definitions for valid temporal status values",
      "bke:statusDefinitions": {
        "current": {
          "label": "Current",
          "description": "Entity is currently valid (validFrom <= now AND (validTo IS NULL OR validTo > now))"
        },
        "expired": {
          "label": "Expired",
          "description": "Entity has passed its validity period (validTo < now)"
        },
        "pending": {
          "label": "Pending",
          "description": "Entity is not yet valid (validFrom > now)"
        },
        "superseded": {
          "label": "Superseded",
          "description": "Entity has been replaced by a newer version (supersededBy IS NOT NULL)"
        }
      }
    },

    {
      "@id": "bke:EntityLifecycleRelationship",
      "@type": "rdfs:Class",
      "label": "Entity Lifecycle Relationship",
      "comment": "Relationships for entity lifecycle, succession, and deprecation tracking",
      "subClassOf": "bke:Relationship"
    },
    {
      "@id": "bke:REPLACED_BY",
      "@type": "rdf:Property",
      "label": "replaced by",
      "comment": "Indicates that the source entity has been replaced by the target entity (e.g., system replacement)",
      "subClassOf": "bke:EntityLifecycleRelationship",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:REPLACES"
    },
    {
      "@id": "bke:REPLACES",
      "@type": "rdf:Property",
      "label": "replaces",
      "comment": "Indicates that the source entity replaces the target entity",
      "subClassOf": "bke:EntityLifecycleRelationship",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:REPLACED_BY"
    },
    {
      "@id": "bke:DEPRECATED_BY",
      "@type": "rdf:Property",
      "label": "deprecated by",
      "comment": "Indicates that the source entity is deprecated by the target entity",
      "subClassOf": "bke:EntityLifecycleRelationship",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:DEPRECATES"
    },
    {
      "@id": "bke:DEPRECATES",
      "@type": "rdf:Property",
      "label": "deprecates",
      "comment": "Indicates that the source entity deprecates the target entity",
      "subClassOf": "bke:EntityLifecycleRelationship",
      "domain": "bke:Entity",
      "range": "bke:Entity",
      "inverseOf": "bke:DEPRECATED_BY"
    },

